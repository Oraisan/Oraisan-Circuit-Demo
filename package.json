    "zkey-AddRH": "snarkjs g16s ./src/block/signature/verifyByBytes/calculateAddRH/*.r1cs ./ptau/powersOfTau28_hez_final_21.ptau ./src/block/signature/verifyByBytes/calculateAddRH/circuit_final.zkey",
    "zkey-PMul1": "snarkjs g16s ./src/block/signature/verifyByBytes/verifyPMul1/*.r1cs ./ptau/powersOfTau28_hez_final_21.ptau ./src/block/signature/verifyByBytes/verifyPMul1/circuit_final.zkey",
    "zkey-encode-message": "snarkjs g16s ./test/message/*.r1cs ./ptau/powersOfTau28_hez_final_13.ptau ./test/message/circuit_final.zkey"

